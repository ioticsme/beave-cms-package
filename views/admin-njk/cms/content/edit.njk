																				<i class="la la-trash-o"></i>Delete</a>
																		</div>
																	</div>
																{% endfor %}
															</div>

															<div class="form-group my-3 text-center">
																<a class="btn btn-light-primary" onClick="cloneChild(event)" target-lang="{{cfg._id}}" target-lang-prefix="{{brandLanguage.prefix}}" target-id="beave_docs_repeater_{{brandLanguage.prefix}}_{{cfg._id}}_{{loop.index}}" data-repeater-create="">
